{
    "authors": [
        {
            "email": "minottoemanuele@gmail.com",
            "homepage": "https://github.com/EmanueleMinotto",
            "name": "Emanuele Minotto",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "EmanueleMinotto\\NaughtyValidator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EmanueleMinotto\\NaughtyValidator\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "description": "Symfony validator for naughty strings",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "homepage": "https://github.com/EmanueleMinotto/naughty-validator",
    "keywords": [
        "symfony",
        "naughty strings",
        "validator"
    ],
    "license": "MIT",
    "name": "emanueleminotto/naughty-validator",
    "require": {
        "php": "^5.6 || ^7.0",
        "symfony/validator": "^2.8 || ^3.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.0",
        "phpunit/phpunit": "^5.4.3"
    },
    "scripts": {
        "test": [
            "php-cs-fixer --ansi -vv fix",
            "phpunit --colors=always"
        ]
    },
    "type": "library"
}
